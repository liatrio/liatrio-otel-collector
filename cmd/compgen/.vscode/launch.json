  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Compgen",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "receiver",
        "github.com/myreceiver",
        "../../receiver/"
      ]
    },
    {
      "name": "Debug Current Test File",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceFolder}/${relativeFileDirname}",
    }
  ]
}